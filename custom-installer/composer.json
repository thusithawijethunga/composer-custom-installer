{
    "name": "thusitha/my-plugin",
    "description": "My Composer plugin",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.3.0",
        "composer/composer": "^2.5.0"
    },
    "autoload": {
        "psr-4": {
            "Thusitha\\": "src/"
        }
    },
    "extra": {
        "class": "Thusitha\\MyPlugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}